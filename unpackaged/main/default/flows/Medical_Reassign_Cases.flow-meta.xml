            <dataType>Date</dataType>
            <defaultValue>
                <elementReference>$Flow.CurrentDate</elementReference>
            </defaultValue>
            <fieldText>Last PRx Outreach Attempt</fieldText>
            <fieldType>InputField</fieldType>
            <helpText>&lt;p&gt;Select the date of the latest telephonic outreach PRx made to either the MBR or HCP&lt;/p&gt;</helpText>
