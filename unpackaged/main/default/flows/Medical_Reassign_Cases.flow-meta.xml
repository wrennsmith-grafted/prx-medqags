                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>New_MBR_Case_Status</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>3rd Attempt Needed</stringValue>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>New_MBR_Case_Status</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>Verify Fax Number</stringValue>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>New_MBR_Case_Status</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>Pending</stringValue>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>New_MBR_Case_Status</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>4th Attempt Needed</stringValue>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>New_MBR_Case_Status</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>5th Attempt Needed</stringValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
